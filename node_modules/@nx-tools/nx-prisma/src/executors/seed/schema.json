{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "Prisma Seed executor",
  "description": "Runs the seed script",
  "type": "object",
  "properties": {
    "script": {
      "type": "string",
      "description": "The path to the seeding script"
    },
    "tsConfig": {
      "type": "string",
      "description": "TypeScript config to use for seeding"
    }
  },
  "required": ["script", "tsConfig"]
}
