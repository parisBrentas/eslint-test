{
  "name": "eslint-test",
  "version": "0.0.1",
  "author": "Paris Brentas",
  "description": "eslint-test",
  "license": "UNLICENSED",
  "private": true,
  "repository": "https://github.com/parisBrentas/eslint-test",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "yarn nx serve",
    "build": "yarn nx build",
    "test": "yarn nx test",
    "testall": "yarn nx run-many --all --target=test --coverage --parallel=8",
    "generate": "nx run-many --all --target=generate",
    "lint": "nx lint",
    "lint-package-json": "npmPkgJsonLint . --quiet",
    "totalcov": "bash ./tools/bulk-move-cov.sh && yarn nyc report --reporter=html-spa -t .totalcov --report-dir=.totalcov",
    "tools:build": "yarn tsc --project tools/tsconfig.json",
    "component:build": "node ./apps/website/scaffold/scaffold.js build",
    "component:destroy": "node ./apps/website/scaffold/scaffold.js destroy",
    "audit-crit": "yarn run improved-yarn-audit --min-severity critical --ignore-dev-deps",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nrwl/devkit": "14.5.10",
    "@nrwl/next": "14.5.10",
    "next": "12.3.2",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.2",
    "react-modal": "^3.15.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-typescript": "7.12.13",
    "@badeball/cypress-cucumber-preprocessor": "^13.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@cdktf/provider-aws": "^10.0.2",
    "@nrwl/cli": "14.5.10",
    "@nrwl/cypress": "^14.7.10",
    "@nrwl/eslint-plugin-nx": "14.5.10",
    "@nrwl/js": "14.5.10",
    "@nrwl/linter": "14.5.10",
    "@nrwl/nest": "14.5.10",
    "@nrwl/next": "14.5.10",
    "@nrwl/node": "14.5.10",
    "@nrwl/nx-cloud": "14.5.4",
    "@nrwl/nx-plugin": "14.5.10",
    "@nrwl/react": "14.5.10",
    "@nrwl/storybook": "14.5.10",
    "@nrwl/web": "14.5.10",
    "@nrwl/webpack": "^15.0.0",
    "@nrwl/workspace": "14.5.10",
    "@nx-tools/nx-prisma": "^2.0.0-alpha.4",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/builder-webpack5": "~6.5.9",
    "@types/node": "18.7.5",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "dotenv-cli": "^5.0.0",
    "esbuild": "^0.15.11",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-newlines": "^1.2.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "moment": "^2.29.4",
    "npm-package-json-lint": "^6.3.0",
    "nx": "14.5.10",
    "ts-loader": "^9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "4.7.4"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true,
    "nonGlobalStepDefinitions": true,
    "step_definitions": "cypress/e2e/step_definitions/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  }
}
